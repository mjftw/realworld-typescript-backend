  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "typescript": "^4.1.2",
    "@types/node": "^14.14.10"
  },
  "dependencies": {
